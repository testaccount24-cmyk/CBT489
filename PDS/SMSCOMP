//useridS  JOB (8231,'LIST DELIVERED SUT ELEMENTS'),
//             '###### COMPARE',
//             CLASS=x,MSGCLASS=x,MSGLEVEL=1,
//             NOTIFY=userid
//*--------------------------------------------------
//SMSCOMP  PROC MEMBR=D##T0A6,     OR     D##HHRR
//             TAPE1=######
//*--------------------------------------------------
//*   !! CHANGE MEMBR (ABOVE) TO MEMBER NAME IN
//*      PLSMS.TMSI.SITE.TAPEDOC.VS######
//*--------------------------------------------------
//*   LOOP THROUGH SUT DELIVERY DOCUMENTATION AND COMPARE WITH
//*   LIST OF CHANGES BY APPLICATIONS - NOTIFY OF MATCHES
//*--------------------------------------------------
//S010     EXEC PGM=IEFBR14,REGION=2048K,
//             COND=(0,NE)
//DASD1    DD  DSN=PDSMS.SUT0.&TAPE1..&MEMBR..ELEMENT.LIST,
//             DISP=(MOD,DELETE),
//             UNIT=SYSDA,SPACE=(TRK,(1,0))
//DASD2    DD  DSN=PDSMS.SUT0.&TAPE1..&MEMBR..APPLY.COMMENT,
//             DISP=(MOD,DELETE),
//             UNIT=SYSDA,SPACE=(TRK,(1,0))
//DASD3    DD  DSN=PDSMS.SUT0.&TAPE1..&MEMBR..PROBLEM.DESC,
//             DISP=(MOD,DELETE),
//             UNIT=SYSDA,SPACE=(TRK,(1,0))
//*--------------------------------------------------
//S020     EXEC PGM=IEFBR14,REGION=2048K,
//             COND=(0,NE)
//SUTO01   DD  DSN=PDSMS.SUT0.&TAPE1..&MEMBR..ELEMENT.LIST,
//             DISP=(NEW,CATLG,KEEP),
//             DATACLAS=D0FBPO,DCB=LRECL=132
//SUTO02   DD  DSN=PDSMS.SUT0.&TAPE1..&MEMBR..APPLY.COMMENT,
//             DISP=(NEW,CATLG,KEEP),
//             DATACLAS=D0FBPO,DCB=LRECL=132
//SUTO03   DD  DSN=PDSMS.SUT0.&TAPE1..&MEMBR..PROBLEM.DESC,
//             DISP=(NEW,CATLG,KEEP),
//             DATACLAS=D0FBPO,DCB=LRECL=132
//*--------------------------------------------------
//S030     EXEC PGM=MIS0480B,REGION=2048K,
//             COND=(0,NE)
//STEPLIB  DD  DISP=SHR,DSN=SYS1.COB2LIB
//SYSOUT   DD  SYSOUT=X
//SYSUDUMP DD  SYSOUT=D
//SYSDBOUT DD  SYSOUT=X
//SYSABOUT DD  SYSOUT=X
//SYSPRINT DD  SYSOUT=X
//SUTIN    DD  DISP=SHR,DSN=PLSMS.TMSI.SITE.TAPEDOC.VS&TAPE1(&MEMBR)
//SUTO01   DD  DSN=PDSMS.SUT0.&TAPE1..&MEMBR..ELEMENT.LIST(ELEMENT),
//             DISP=SHR
//SUTO02   DD  DSN=PDSMS.SUT0.&TAPE1..&MEMBR..APPLY.COMMENT(COMMENT),
//             DISP=SHR
//SUTO03   DD  DSN=PDSMS.SUT0.&TAPE1..&MEMBR..PROBLEM.DESC(PROBLEM),
//             DISP=SHR
//SMSNAME  DD  DSN=PLSMS.TMSI.SITE.APPLYJCL.VS&TAPE1(CPY2PROD),
//             DISP=SHR
//*--------------------------------------------------
//S040     EXEC PGM=SORT,REGION=4096K,
//             COND=(0,NE)
//SORTMSG  DD  SYSOUT=X
//SYSOUT   DD  SYSOUT=X
//SYSUDUMP DD  SYSOUT=D
//SORTIN   DD  DSN=PDSMS.SUT0.&TAPE1..&MEMBR..ELEMENT.LIST(ELEMENT),
//             DISP=SHR
//SORTOUT  DD  DSN=&&DELIVER,                          *OUTPUT
//             DISP=(NEW,PASS,DELETE),
//             UNIT=SYSDA,SPACE=(CYL,(10,1),RLSE),
//             DCB=(RECFM=FB,LRECL=132,DSORG=PS)
//SYSIN    DD  DISP=SHR,DSN=userid.ISPF.FILE489(SMSCMP01)
//SORTWK01 DD  UNIT=SYSDA,SPACE=(CYL,(30,2))
//SORTWK02 DD  UNIT=SYSDA,SPACE=(CYL,(30,2))
//SORTWK03 DD  UNIT=SYSDA,SPACE=(CYL,(30,2))
//*--------------------------------------------------
//S050     EXEC PGM=SORT,REGION=4096K,
//             COND=(0,NE)
//SORTMSG  DD  SYSOUT=X
//SYSOUT   DD  SYSOUT=X
//SYSUDUMP DD  SYSOUT=D
//SORTIN   DD  DSN=ADMP.DOCUMENT.LIBRARY(SMSSUT),
//             DISP=SHR
//SORTOUT  DD  DSN=&&SMSSUT,                           *OUTPUT
//             DISP=(NEW,PASS,DELETE),
//             UNIT=SYSDA,SPACE=(CYL,(10,1),RLSE),
//             DCB=(RECFM=FB,LRECL=80,DSORG=PS)
//SYSIN    DD  DISP=SHR,DSN=userid.ISPF.FILE489(SMSCMP02)
//SORTWK01 DD  UNIT=SYSDA,SPACE=(CYL,(30,2))
//SORTWK02 DD  UNIT=SYSDA,SPACE=(CYL,(30,2))
//SORTWK03 DD  UNIT=SYSDA,SPACE=(CYL,(30,2))
//*--------------------------------------------------
//S060     EXEC PGM=SORT,REGION=4096K,
//             COND=(0,NE)
//SORTMSG  DD  SYSOUT=X
//SYSOUT   DD  SYSOUT=X
//SYSUDUMP DD  SYSOUT=D
//SORTIN   DD  DSN=ADMP.DOCUMENT.LIBRARY(SMSSUT),
//             DISP=SHR
//SORTOUT  DD  DSN=&&SMSSUT2,                          *OUTPUT
//             DISP=(NEW,PASS,DELETE),
//             UNIT=SYSDA,SPACE=(CYL,(10,1),RLSE),
//             DCB=(RECFM=FB,LRECL=80,DSORG=PS)
//SYSIN    DD  DISP=SHR,DSN=userid.ISPF.FILE489(SMSCMP02)
//SORTWK01 DD  UNIT=SYSDA,SPACE=(CYL,(30,2))
//SORTWK02 DD  UNIT=SYSDA,SPACE=(CYL,(30,2))
//SORTWK03 DD  UNIT=SYSDA,SPACE=(CYL,(30,2))
//*--------------------------------------------------
//S070     EXEC PGM=MIS0485B,
//             REGION=2048K,
//             COND=(0,NE)
//STEPLIB  DD  DISP=SHR,DSN=SYS1.COB2LIB
//SYSOUT   DD  SYSOUT=X
//SYSUDUMP DD  SYSOUT=D
//SYSDBOUT DD  SYSOUT=X
//SYSABOUT DD  SYSOUT=X
//SYSPRINT DD  SYSOUT=X
//DELIVER  DD  DSN=&&DELIVER,DISP=(OLD,DELETE)
//SMSSUT   DD  DSN=&&SMSSUT,DISP=(OLD,DELETE)
//SMSSUT2  DD  DSN=&&SMSSUT2,DISP=(OLD,DELETE)
//PRINTER  DD  SYSOUT=(Y,MIS0485B,8X11),
//             FCB=X116,DCB=RECFM=FM
//*--------------------------------------------------
//S080     EXEC PGM=PULL0132,
//             REGION=2048K,
//             COND=(0,NE),
//             PARM='0132NN44  /'
//STEPLIB  DD  DISP=SHR,DSN=SYS1.COB2LIB
//SYSOUT   DD  SYSOUT=X
//SYSUDUMP DD  SYSOUT=D
//SYSPRINT DD  SYSOUT=X
//INFILE   DD  DSN=PDSMS.SUT0.&TAPE1..&MEMBR..PROBLEM.DESC(PROBLEM),
//             DISP=SHR
//OUTFILE  DD  DUMMY
//OUTPRNT  DD  SYSOUT=(Y,SISTDAP2,STD),
//             FCB=STD,DEST=LOCAL
//PARM01   DD  DISP=SHR,DSN=userid.ISPF.FILE489(SMSPUL99)
//*--------------------------------------------------
//         PEND
//J010     EXEC SMSCOMP

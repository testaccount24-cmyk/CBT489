//useridS  JOB (8231,'CLEAR SMS CICS LIBRARIES'),
//             '###### CLEAR CICS',
//*            RESTART=J010.S040,
//             CLASS=x,MSGCLASS=x,MSGLEVEL=1
//*--------------------------------------------------
//*    EDIT TO ONLY INCLUDE LIBRARIES MENTIONED IN SUT APPLY
//*--------------------------------------------------
//S010     EXEC PGM=IEBCOPY,REGION=4096K
//SYSPRINT DD SYSOUT=X
//OUT1     DD  DISP=SHR,DSN=PLSMS.CICVNDRL.P0410.COBLIB
//OUT2     DD  DISP=SHR,DSN=PLSMS.CICVNDRL.P0410.MACLIB
//OUT3     DD  DISP=SHR,DSN=PLSMS.CICVNDRL.P0410.PL1LIB
//SYSUT3   DD  UNIT=SYSDA,SPACE=(CYL,15)
//SYSUT4   DD  UNIT=SYSDA,SPACE=(CYL,15)
//SYSIN    DD *
  EMPTYO   OUTDD=OUT1
  EMPTYO   OUTDD=OUT2
  EMPTYO   OUTDD=OUT3
/*
//*--------------------------------------------------
//S020     EXEC PGM=IEBCOPY,REGION=4096K,
//             COND=(0,NE)
//SYSPRINT DD SYSOUT=X
//OUT1     DD  DISP=SHR,DSN=PLSMS.CICVNDRL.P0410.LOADLIB
//OUT2     DD  DISP=SHR,DSN=PLSMS.CICVNDRL.P0410.LOADLIB1
//SYSUT3   DD  UNIT=SYSDA,SPACE=(CYL,15)
//SYSUT4   DD  UNIT=SYSDA,SPACE=(CYL,15)
//SYSIN    DD *
  EDITDIR   OUTDD=OUT1
    DELETE    MEMBER=DFH*
  EDITDIR   OUTDD=OUT2
    DELETE    MEMBER=DFH*
/*
//*--------------------------------------------------
//S030     EXEC PGM=IEBCOPY,REGION=4096K,
//             COND=(0,NE)
//SYSPRINT DD SYSOUT=X
//OUT1     DD  DISP=SHR,DSN=PLSMS.CICVNDRL.P0410.LOADLIB
//OUT2     DD  DISP=SHR,DSN=PLSMS.CICVNDRL.P0410.LOADLIB1
//OUT3     DD  DISP=SHR,DSN=PLSMS.CICVNDRL.P0410.COBLIB
//OUT4     DD  DISP=SHR,DSN=PLSMS.CICVNDRL.P0410.MACLIB
//OUT5     DD  DISP=SHR,DSN=PLSMS.CICVNDRL.P0410.PL1LIB
//SYSUT3   DD  UNIT=SYSDA,SPACE=(CYL,15)
//SYSUT4   DD  UNIT=SYSDA,SPACE=(CYL,15)
//SYSIN    DD *
  COMPRESS  OUTDD=OUT1
  COMPRESS  OUTDD=OUT2
  COMPRESS  OUTDD=OUT3
  COMPRESS  OUTDD=OUT4
  COMPRESS  OUTDD=OUT5
/*
//*--------------------------------------------------
//S040     EXEC PGM=IEBCOPY,REGION=4096K,
//             COND=(0,NE)
//SYSPRINT DD SYSOUT=X
//IN1      DD  DISP=SHR,DSN=PLSMS.PLSMS.CICVNDRL.P0410.LOADLIB
//OUT2     DD  DISP=SHR,DSN=PLSYS.SMLSM00L.E0000.LOAD
//SYSUT3   DD  UNIT=SYSDA,SPACE=(CYL,15)
//SYSUT4   DD  UNIT=SYSDA,SPACE=(CYL,15)
//SYSIN    DD *
  COPYMOD  INDD=((IN1,R)),OUTDD=OUT1,COPYALIAS
    SELECT MEMBER=DFHDU410
    SELECT MEMBER=DFHTU410
    SELECT MEMBER=DFHTR410
    SELECT MEMBER=DFHPD410
    SELECT MEMBER=DFHSTUP
    SELECT MEMBER=DFHTG410
/*
//*--------------------------------------------------

//useridS  JOB (8231,'MANAGE SMS LOAD LIBRARIES'),
//             '###### LOAD LIBS',
//*            RESTART=J010.S040,
//             CLASS=x,MSGCLASS=x,MSGLEVEL=1
//*--------------------------------------------------
//*    EDIT TO ONLY INCLUDE LIBRARIES DELIVERED ON THE SUT TAPE
//*--------------------------------------------------
//*    BE SURE EACH FILES TRACK AND DIRECTORY SPACE WILL FIT IN
//*    EACH TARGET LIBRARY
//*--------------------------------------------------
//S010     EXEC PGM=IEBCOPY,REGION=4096K
//SYSPRINT DD SYSOUT=X
//IN1      DD  DISP=SHR,DSN=PLSMS.SMBSM00L.E0000.LOAD
//IN2      DD  DISP=SHR,DSN=PLSMS.SMOSM00L.E0000.LOAD
//IN3      DD  DISP=SHR,DSN=PLSMS.SMSCUSTL.E0000.LOAD
//IN4      DD  DISP=SHR,DSN=PLSMS.CICSM01L.E0410.LOAD
//IN5      DD  DISP=SHR,DSN=PLSYS.SMLSM00L.E0000.LOAD
//IN6      DD  DISP=SHR,DSN=PLSMS.SMSSM00L.E0000.DBRM
//*
//OUT1     DD  DISP=SHR,DSN=PLSMS.SMBSM00L.P0000.LOAD
//OUT2     DD  DISP=SHR,DSN=PLSMS.SMOSM00L.P0000.LOAD
//OUT3     DD  DISP=SHR,DSN=PLSMS.SMSCUSTL.P0000.LOAD
//OUT4     DD  DISP=SHR,DSN=PLSMS.CICSM01L.P0410.LOAD
//OUT5     DD  DISP=SHR,DSN=PLSYS.SMLSM00L.P0000.LOAD
//OUT6     DD  DISP=SHR,DSN=PLSMS.SMSSM00L.P0000.DBRM
//SYSUT3   DD  UNIT=SYSDA,SPACE=(CYL,15)
//SYSUT4   DD  UNIT=SYSDA,SPACE=(CYL,15)
//SYSIN    DD *
  COPYMOD INDD=((IN1,R)),OUTDD=OUT1
  EXCLUDE MEMBER=$$$SPACE
  COPYMOD INDD=((IN2,R)),OUTDD=OUT2
  EXCLUDE MEMBER=$$$SPACE
  COPYMOD INDD=((IN3,R)),OUTDD=OUT3
  EXCLUDE MEMBER=$$$SPACE
  COPYMOD INDD=((IN4,R)),OUTDD=OUT4
  EXCLUDE MEMBER=$$$SPACE
  COPYMOD INDD=((IN5,R)),OUTDD=OUT5
  EXCLUDE MEMBER=$$$SPACE
  COPY    INDD=((IN6,R)),OUTDD=OUT6
  EXCLUDE MEMBER=$$$SPACE
/*
//*--------------------------------------------------
//S020     EXEC PGM=IEBCOPY,REGION=4096K,
//             COND=(4,LT)
//SYSPRINT DD SYSOUT=X
//IN1      DD  DISP=SHR,DSN=PLSMS.SMBSM00L.E0000.LOAD
//IN2      DD  DISP=SHR,DSN=PLSMS.SMOSM00L.E0000.LOAD
//IN3      DD  DISP=SHR,DSN=PLSMS.SMSCUSTL.E0000.LOAD
//IN4      DD  DISP=SHR,DSN=PLSMS.CICSM01L.E0410.LOAD
//IN5      DD  DISP=SHR,DSN=PLSYS.SMLSM00L.E0000.LOAD
//IN6      DD  DISP=SHR,DSN=PLSMS.SMSSM00L.E0000.DBRM
//SYSUT3   DD  UNIT=SYSDA,SPACE=(CYL,15)
//SYSUT4   DD  UNIT=SYSDA,SPACE=(CYL,15)
//SYSIN    DD *
  EMPTYO  INDD=IN1
  EMPTYO  INDD=IN2
  EMPTYO  INDD=IN3
  EMPTYO  INDD=IN4
  EMPTYO  INDD=IN5
  EMPTYO  INDD=IN6
/*
//*--------------------------------------------------
//S030     EXEC PGM=IEBCOPY,REGION=4096K,
//             COND=(4,LT)
//SYSPRINT DD SYSOUT=X
//OUT1     DD  DISP=SHR,DSN=PLSMS.SMBSM00L.P0000.LOAD
//OUT2     DD  DISP=SHR,DSN=PLSMS.SMOSM00L.P0000.LOAD
//OUT3     DD  DISP=SHR,DSN=PLSMS.SMSCUSTL.P0000.LOAD
//OUT4     DD  DISP=SHR,DSN=PLSMS.CICSM01L.P0410.LOAD
//OUT6     DD  DISP=SHR,DSN=PLSMS.SMSSM00L.P0000.DBRM
//SYSUT3   DD  UNIT=SYSDA,SPACE=(CYL,15)
//SYSUT4   DD  UNIT=SYSDA,SPACE=(CYL,15)
//SYSIN    DD *
  COMPRESS OUTDD=OUT1
  COMPRESS OUTDD=OUT2
  COMPRESS OUTDD=OUT3
  COMPRESS OUTDD=OUT4
  COMPRESS OUTDD=OUT6
/*
//*--------------------------------------------------
